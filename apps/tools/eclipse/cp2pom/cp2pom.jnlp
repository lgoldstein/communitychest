   		<jar href="lib/net.community-maven-helpers.jar"/>
   		<jar href="lib/net.community-eclipse.jar"/>
   		<jar href="lib/net.community-gui-helpers.jar"/>   
   		<jar href="lib/net.community-gui-awt.jar"/>   
   		<jar href="lib/net.community-gui-swing.jar"/>   
   		<jar href="lib/net.community-gui-components.jar"/>   
   		<jar href="lib/net.community-logging.jar"/>   
   		<jar href="lib/net.community-io.jar"/>   
   		<jar href="lib/net.community-jdk.jar"/>
		<j2se version="1.6+" href="http://java.sun.com/products/autodl/j2se"/>
   </resources>
   <application-desc main-class="net.community.apps.eclipse.cp2pom.Main"/>
</jnlp>
