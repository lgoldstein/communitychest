<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for Eclipse classpath file arranger -->
<jnlp spec="1.0+" codebase="$$codebase" href="$$name">
   <information>
      	<title>CP2POM</title>
      	<vendor>Net-Community-Chest: Lyor G.</vendor>
      	<description>Creates a Maven dependencies XML from an Eclipse classpath file</description>
      	<homepage href="http://java.sun.com/docs/books/tutorial/deployment/webstart/running.html"/>
