       </dependency>
       <dependency>
           <groupId>org.apache.tomcat</groupId>
           <artifactId>juli</artifactId>
           <version>${tomcat.version}</version>
       </dependency>
       <dependency>
           <groupId>org.apache.tomcat</groupId>
           <artifactId>jasper</artifactId>
           <version>${tomcat.version}</version>
       </dependency>
    </dependencies>
</project>
